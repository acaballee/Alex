{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "circuit": {
      "type": "string",
      "enum": ["Prado Rosquilla", "Cataratas Yoshi", "Barranco Goloso", "FÃ¡brica de Toad"]
    },
    "classificacio": {
      "type": "array",
      "minItems": 2,
      "maxItems": 20,
      "items": {
        "type": "object",
        "properties": {
          "posicio": {
            "type": "integer",
            "minimum": 1,
            "maximum": 20
          },
          "nick-player": {
            "type": "string"
          },
          "personatge": {
            "type": "string"
          },
          "color": {
            "type": "string",
            "enum": ["blanc", "negre", "groc", "blau", "roig", "verd", "taronja", "lila", "rosa"]
          },
          "Temps": {
            "type": "string",
            "pattern": "^\\d{1,2}:\\d{2}$"
          },
          "voltes": {
            "type": "integer",
            "minimum": 0,
            "maximum": 5
          },
          "Accidents": {
            "type": "integer"
          },
          "Nitros": {
            "type": "integer"
          },
          "Rookie": {
            "type": "string",
            "enum": ["SI", "NO"]
          }
        },
        "required": ["posicio", "nick-player", "personatge", "Temps", "voltes", "Accidents"],
        "additionalProperties": false
      }
    }
  },
  "required": ["classificacio"],
  "additionalProperties": false
}

